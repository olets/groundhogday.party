{
  "name": "groundhogday.party",
  "description": "Source for groundhogday.party",
  "version": "1.0.0",
  "main": "index.js",
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-htmlmin": "^5.0.1",
    "gulp-twing": "^4.0.0",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.18",
    "twing": "^5.1.0"
  },
  "scripts": {
    "ci": "npm run ci:build && npm run ci:styles",
    "ci:build": "npx gulp build",
    "ci:clean": "npx gulp clean",
    "ci:styles": "npx tailwindcss -i ./src/styles/input.css -o ./dist/output.css --minify",
    "dev": "pnpm dev:build && pnpm dev:styles",
    "dev:build": "pnpm gulp build",
    "dev:clean": "pnpm gulp clean",
    "dev:styles": "npx tailwindcss -i ./src/styles/input.css -o ./dist/output.css --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olets/groundhogday.party.git"
  },
  "author": "Henry Bley-Vroman <olets@olets.dev>",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/olets/groundhogday.party/issues"
  },
  "homepage": "https://github.com/olets/groundhogday.party#readme"
}
